pipeline {
   agent any
   stages {
      stage('Hello') {
         steps {
            echo 'Hello World'
         }
      }
      stage('Welcome'){
         steps{
             echo 'Welcome ya 3amoora :D'
             
         }
      }
      stage('Ahleen'){
          steps{
              echo('Ahleen 3amoora')
          }
      }     
   }
}
